pipeline {
    agent {label'GRADLE && JDK11 && NVM && PY3'}
    stages {
        stage('clone') {
           steps {
            git url:'https://github.com/QtBhavani/shopizer.git' , branch :'SPRINT'
            }
        }
        stage('install') {
            steps {
            sh 'mvn package'
            }
        }
    }
}