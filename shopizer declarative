pipeline {
    agent {label 'GRADLE && JDK11 && NVM && PY3' }
    triggers {
        cron('30 17 * * *')
    stages {
        stage('clone') {
            steps {
                git url:'https://github.com/QtBhavani/shopizer.git' , branch :'release'
            }
        }
        stage('install') { 
            steps { 
                sh 'mvn package'
            }

        }
    }
}