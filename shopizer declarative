pipeline {
    agent {label 'MVN3 && JDK11 && NVM && PY3' }
    triggers {
        pollSCM('* * * * *')
    stages {
        stage('clone') {
            steps {
                git url:'https://github.com/QtBhavani/shopizer.git' , branch :'develop'
            }
        }
        stage('install') { 
            steps { 
                sh 'mvn package'
            }

        }
    }
}
}